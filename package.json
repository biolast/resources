{
	"name": "@biolast/resources",
	"version": "1.0.3",
	"description": "Resources used for biolast",
	"author": "blobfysh",
	"license": "MIT",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "rollup --config rollup.config.js",
		"prepublishOnly": "yarn run build",
		"lint": "./node_modules/.bin/eslint . --ext .ts,.js,.json",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest"
	},
	"exports": {
		"types": "./dist/index.d.ts",
		"import": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"engines": {
		"node": ">=18"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/biolast/resources.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@blobfysh/eslint-config": "^1.0.6",
		"@rollup/plugin-dynamic-import-vars": "^2.0.3",
		"@rollup/plugin-image": "^3.0.2",
		"@rollup/plugin-typescript": "^11.1.1",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.1.7",
		"jest": "^29.5.0",
		"rimraf": "^5.0.0",
		"rollup": "^3.22.0",
		"rollup-plugin-copy": "^3.4.0",
		"ts-jest": "^29.1.0",
		"ts-jest-resolver": "^2.0.1",
		"typescript": "^5.0.4"
	},
	"jest": {
		"resolver": "ts-jest-resolver",
		"transform": {
			"^.+\\.ts$": [
				"ts-jest",
				{
					"useESM": true
				}
			],
			"^.+\\.png$": "<rootDir>/tests/imageTransformer.cjs"
		}
	},
	"packageManager": "yarn@3.6.0"
}
